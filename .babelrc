    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "browsers": ["last 2 Chrome versions"]
          }
        }
      ],
      [
        "@babel/preset-react",
        { "runtime": "automatic", "importSource": "@emotion/react" }
      ]
    ],
    "plugins": ["@emotion/babel-plugin"]
  }