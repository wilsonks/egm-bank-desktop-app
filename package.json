{
  "name": "egm-bank-desktop-app",
  "version": "1.0.0",
  "description": "Template for an electron desktop app(400x800px) built with react + redux ",
  "main": "release/app/dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "start:watch": "nodemon --watch ./release/app/dist --exec \"electron . \"",
    "package": "npm run build:main && electron-builder --publish never",
    "package:windows": "npm run build:main && electron-builder --windows  --publish never",
    "package:linux": "npm run build:main && electron-builder --linux  --publish never",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.main.ts",
    "build:main:watch": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.main.ts --watch",
    "build:renderer": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.ts",
    "build:renderer:watch": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.ts --watch",
    "clean:renderer": "ts-node ./scripts/clean.js  dist/renderer",
    "clean:main": "ts-node ./scripts/clean.js  dist/main",
    "clean": "ts-node ./scripts/clean.js  dist"
  },
  "author": "wilson@wildace.in",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@electron/rebuild": "^3.2.13",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^25.0.1",
    "electron-better-ipc": "^2.0.1",
    "electron-builder": "^23.6.0",
    "execa": "^7.1.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.3",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.10.4",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.10.0",
    "@reduxjs/toolkit": "^1.9.5",
    "chalk": "^5.4.1",
    "framer-motion": "^6.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.11.2",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^2.0.0",
    "triple-beam": "^1.4.1",
    "util": "^0.12.5",
    "winston": "^3.17.0",
    "yup": "^1.6.1"
  },
  "build": {
    "productName": "Cage App",
    "copyright": "Copyright Â© 2025 WildAce",
    "appId": "org.wilace.cage",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ]
  }
}
